    <% records.forEach(function(records) { %>
  <div class="container">
    <div class="card" style="width: 18rem;">
      <img src="<%= records.img %> " alt="Image" width="auto" height="220">
      <div class="card-body">
        <h5 class="card-title"><strong><%= records.name %></strong></h5>
        <p class="card-text"><%= records.Desc %></p>
        <a href="<%= records.id %>" class="btn btn-primary">Show</a>
        <!-- <a href="<%= records.id %>" class="btn btn-primary">Show</a> -->
      </div>
    </div>
  </div>
    <% }); %> 


    <script>
      $(function(){
    $('#searchName').autocomplete({
    source: function(req,res){
      $.ajax({
        url:"autocomplete/",
        dataType:"jsonp",
        type:"GET",
        data:req,
        success: function(data){
          res(data)
        },
        error: function(err){
          console.log(err.status);
        }
      });
    },
    minLength:1,
    select: function(event,ui){
      if(ui.item){
        $('#searchName').val(ui.item.label);
      }
    }
    });
      });
      </script>
  </body>
